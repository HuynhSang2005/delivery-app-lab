{
  "name": "@logship/api",
  "version": "0.1.0",
  "description": "Logship MVP API â€” NestJS backend for hyperlocal delivery",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "bunx nest build",
    "format": "bunx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "bunx prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bunx nest start",
    "start:dev": "bunx nest start --watch",
    "start:debug": "bunx nest start --debug --watch",
    "start:prod": "bun dist/main.js",
    "lint": "bunx eslint \"src/**/*.ts\" \"test/**/*.ts\" --cache --fix",
    "lint:check": "bunx eslint \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "test": "bunx jest",
    "test:watch": "bunx jest --watch",
    "test:cov": "bunx jest --coverage",
    "test:debug": "bun --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "bunx jest --config ./test/jest-e2e.json",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.13",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/platform-socket.io": "^11.1.13",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.13",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "bullmq": "^5.69.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cloudinary": "^2.9.0",
    "firebase-admin": "^13.6.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "nestjs-zod": "^5.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^10.0.1",
    "@golevelup/ts-jest": "^1.2.1",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.13",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.0.0",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.8.1",
    "prisma": "^7.4.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["../test/jest-setup.ts"]
  }
}
